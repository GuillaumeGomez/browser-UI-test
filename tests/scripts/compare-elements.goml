screenshot-comparison: false
goto: file://|CURRENT_DIR|/|DOC_PATH|/elements.html
compare-elements-text: (".content>.right>p", ".content>.left>p")
compare-elements-position: (".content>.right>p", ".content>.right>p", ("x", "y"))
compare-elements-attribute: (".content>.right", ".content>.left", ["data-whatever"])
compare-elements-css: (".content>.right>p", ".content>.left>p", ["margin"])
compare-elements-text-false: (".content>.right>p", ".content>.left")
compare-elements-position-false: (".content>.right>div", ".content>.left>div", ("x", "y"))
compare-elements-attribute-false: (".content>.right", ".content>.left>p", ["data-whatever"])
compare-elements-css-false: (".content>.right>p", ".content>.left", ["margin"])
compare-elements-property-false: (".content>.right", ".content>.left", ["childElementCount"])
compare-elements-property: (".right>div", ".left>div", ["childElementCount"])
fail: true // check that the command fails if the comparison succeeds
compare-elements-text-false: (".content>.right>p", ".content>.left>p")
compare-elements-position-false: (".content>.right>div", ".content>.right>div", ("x", "y"))
compare-elements-attribute-false: (".content>.right", ".content>.right", ["data-whatever"])
compare-elements-css-false: (".content>.right>p", ".content>.right>p", ["margin"])
compare-elements-property: (".right>div", ".left", ["childElementCount"])
